<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title><%- pageTitle %></title>

    <!-- Favicon -->
    <link rel="shortcut icon" href="/img/logo/icon.png" type="image/x-icon">

    <!-- Font awesome -->
    <link href="/css/font-awesome.css" rel="stylesheet">
    <!-- Bootstrap -->
    <link href="/css/bootstrap.css" rel="stylesheet">
    <!-- slick slider -->
    <link rel="stylesheet" type="text/css" href="/css/slick.css">
    <!-- price picker slider -->
    <link rel="stylesheet" type="text/css" href="/css/nouislider.css">
    <!-- Fancybox slider -->
    <link rel="stylesheet" href="/css/jquery.fancybox.css" type="text/css" media="screen"/>
    <!-- Theme color -->
    <link id="switcher" href="/css/theme-color/dark-red-theme.css" rel="stylesheet">
    <!-- Main style sheet -->
    <link href="/css/style.css" rel="stylesheet">


    <!-- Google Font -->
    <link href='https://fonts.googleapis.com/css?family=Vollkorn' rel='stylesheet' type='text/css'>
    <link href='https://fonts.googleapis.com/css?family=Open+Sans' rel='stylesheet' type='text/css'>


    <!-- HTML5 shim and Respond.js for IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    <!--[if lt IE 9]>
    <script src="https://oss.maxcdn.com/html5shiv/3.7.2/html5shiv.min.js"></script>
    <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
    <![endif]-->

    <script src="/js/sweetalert2.all.min.js"></script>

</head>
<body>

<!-- Pre Loader -->
<div id="aa-preloader-area">
    <div class="pulse"></div>
</div>
<!-- SCROLL TOP BUTTON -->
<a class="scrollToTop" href="#"><i class="fa fa-angle-double-up"></i></a>
<!-- END SCROLL TOP BUTTON -->


<!-- Start header section -->

<%- include('navbar.ejs') %>
<!-- End header section -->
<!-- Start menu section -->
<section id="aa-menu-area">
    <nav class="navbar navbar-default main-navbar" role="navigation">
        <div class="container">
            <div class="navbar-header">
                <!-- FOR MOBILE VIEW COLLAPSED BUTTON -->
                <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#navbar"
                        aria-expanded="false" aria-controls="navbar">
                    <span class="sr-only">Toggle navigation</span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                <!-- LOGO -->
                <!-- Text based logo -->
                <!--          <a class="navbar-brand aa-logo" href="/"> Home <span>Property</span></a>-->
                <!-- Image based logo -->
                <a class="navbar-brand aa-logo-img" href="/"><img id="logo-img" src="/img/logo/cropped.png" alt="logo"></a>
            </div>
            <div id="navbar" class="navbar-collapse collapse">
                <ul id="top-menu" class="nav navbar-nav navbar-right aa-main-nav">
                    <li><a href="/">HOME</a></li>
                    <li><a href="/properties">PROPERTIES</a></li>
                    <li><a href="/contact">CONTACT</a></li>
                </ul>
            </div><!--/.nav-collapse -->
        </div>
    </nav>
</section>
<!-- End menu section -->

<!-- Start Proerty header  -->
<section id="aa-property-header">
    <div class="container">
        <div class="row">
            <div class="col-md-12">
                <div class="aa-property-header-inner">
                    <h2>My Profile</h2>
                    <ol class="breadcrumb">
                        <li><a href="#">HOME</a></li>
                        <li class="active">MY PROFILE</li>
                    </ol>
                </div>
            </div>
        </div>
    </div>
</section>
<!-- End Proerty header  -->

<!-- Start Blog  -->
<section id="aa-blog">
    <div class="container">
        <div class="row">
            <div class="col-md-12">
                <div class="aa-blog-area">
                    <div class="row">
                        <!-- Start blog sidebar -->
                        <div class='col-md-2'></div>
                        <div class="col-md-8">
                            <aside class="aa-blog-sidebar">

                                <!-- Start single sidebar -->
                                <div class="aa-blog-sidebar-single">
                                    <h3>User Info</h3>
                                    <ul class="aa-blog-catg" id="dataList">
                                        <li>Name                <span><%- locals.user.name %>   </span>       </li>
                                        <li>Email               <span><%- locals.user.email %>   </span>       </li>
                                        <li>Date of Birth       <span><%- locals.user.dateOfBirth.toLocaleDateString() %>   </span>       </li>
                                        <li>Address             <span><%- locals.user.address %>   </span>       </li>
                                        <li>Phone Number        <span><%- locals.user.phoneNumber %>   </span>       </li>
                                        <li>Nationality         <span><%- locals.user.nationality %>   </span>       </li>
                                        <li>NationalID          <span><%- locals.user.nationalID %>   </span>       </li>
                                        <li>Job                 <span><%- locals.user.job %>   </span>       </li>
                                        <li>Salary              <span><%- locals.user.salary %>   </span>       </li>
                                        <li>MaritalStatus       <span><%- locals.user.maritalStatus %>   </span>       </li>
                                        <li>Gender              <span><%- locals.user.gender %>   </span>       </li>
                                    </ul>
                                </div>

                                <!-- Start single sidebar -->
                                <div class="aa-blog-sidebar-single" style="text-align: center">

                                    <input class="aa-secondary-btn" type="submit" value="Edit Info" id="edit-data-btn"
                                           style="width: 30%; margin: 1rem; border: 0px; border-radius: 0.5rem;">

                                    <input class="aa-secondary-btn" type="submit" value="Deactivate account"
                                           id="deactive-btn"
                                           style="width: 30%; margin: 1rem; border: 0px; border-radius: 0.5rem;">

                                </div>

                                <!-- Start single sidebar -->
                                <div class="aa-blog-sidebar-single">
                                    <h3>Password Change</h3>
                                    <ul class="aa-blog-catg" id="dataList">
                                        <li>Current Password           <span><input type="password" placeholder="Current Password" name="currentPassword" id="current-password"></span>       </li>
                                        <li>New Password               <span><input type="password" placeholder="New Password" name="newPassword" id="new-password"></span>       </li>
                                        <li>New Password Confirm       <span><input type="password" placeholder="New Password Confirm" name="newPasswordConfirm" id="new-password-confirm"></span>       </li>
                                    </ul>

                                    <div style="width: 100%; text-align: center">
                                        <input class="aa-secondary-btn" type="submit" value="Change Password" id="edit-password-btn"
                                               style="width: 30%;border: 0px; border-radius: 0.5rem; margin-top: 1rem">
                                    </div>

                                    <p id="message-response" style="display: none; color: red; text-align: center">Test</p>

                                </div>

                                <!-- Start single sidebar -->
                                <div class="aa-blog-sidebar-single">

                                    <h3>Saved Properties</h3>
                                    <div class="aa-blog-recent-post">

                                        <% locals.user.savedProperties.forEach(property=>{ %>

                                            <div class="media">
                                                <div class="media-left">
                                                    <a href="#">
                                                        <img alt="img" src="<%- property.images[0]%>" class="media-object">
                                                    </a>
                                                </div>
                                                <div class="media-body" style="width: 85%">
                                                    <h4 class="media-heading"><a href="#"><%- property.name%></a></h4>
                                                    <p><%- property.description%></p>
                                                    <span><%- property.price.toLocaleString()%></span>
                                                </div>
                                                <div class="media-right" style="width: 20%">
                                                    <a href="/my-profile" id="<%- property._id%>" class="aa-read-mor-btn"><span
                                                                class="fa fa-trash"></span> Remove</a>
                                                </div>
                                            </div>

                                        <% })%>

                                    </div>
                                </div>

                            </aside>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>
<!-- / Blog  -->

<!-- Footer -->
<%- include('footer.ejs') %>
<!-- / Footer -->

<!-- jQuery library -->
<!--<script src="https://ajax.googleapis.com/ajax/libs/jquery/1.11.3/jquery.min.js"></script> -->
<script src="/js/jquery.min.js"></script>
<!-- Include all compiled plugins (below), or include individual files as needed -->
<script src="/js/bootstrap.js"></script>
<!-- slick slider -->
<script type="text/javascript" src="/js/slick.js"></script>
<!-- Price picker slider -->
<script type="text/javascript" src="/js/nouislider.js"></script>
<!-- mixit slider -->
<script type="text/javascript" src="/js/jquery.mixitup.js"></script>
<!-- Add fancyBox -->
<script type="text/javascript" src="/js/jquery.fancybox.pack.js"></script>
<!-- Custom js -->
<script src="/js/custom.js"></script>
<!-- Navigation logo -->
<script type="text/javascript" src="/js/navimage.js"></script>
<script type="text/javascript" src="js/logout.js"></script>

<script type="text/javascript" src="/js/userPage.js"></script>

</body>
</html>
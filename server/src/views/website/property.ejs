                    <!-- Start Single properties sidebar -->
                    <div class="aa-properties-single-sidebar">
                        <h3>Request More Property Details</h3>
                        <h5>Send Us an inquiry request and we will call you</h5>
                        <span id="property-id" style="display: none"><%- property._id %></span>
                        <% if (locals.user) { %>
                                <span id="user-id" style="display: none"><%- locals.user._id %></span>
                        <% } %>
                        <form action="" id="inquiry-form">
                            <div class="aa-single-advance-search">
                                <input name="message" type="text" placeholder="Type Your Message"
                                       style="height: 200px; font-size: 14px">
                            </div>
                            <div class="aa-single-advance-search">
                                <input type="submit" value="Inquire about this property" class="aa-search-btn"
                                       id="inquiry-btn">
                            </div>
                            <p id="inquiry-message" style="color: red; text-align: center; display: none"></p>
                        </form>
                    </div>
                    <!-- Start Single properties sidebar -->
                    <div class="aa-properties-single-sidebar">
                        <h3>Populer Properties</h3>

                        <% popularProperties.forEach( (popularProp) => { %>
                            <div class="media">
                                <div class="media-left">
                                    <a href="/properties/<%- popularProp.slug %>">
                                        <img class="media-object"
                                             src="<%- popularProp.images[0] %>?random=<%- popularProp.counter %>"
                                             alt="img">
                                    </a>
                                </div>
                                <div class="media-body">
                                    <h4 class="media-heading"><a
                                                href="/properties/<%- popularProp.slug %>"><%- popularProp.name %></a>
                                    </h4>
                                    <p><%- popularProp.description %></p>
                                    <span><%- popularProp.price.toLocaleString() %> EGP</span>
                                </div>
                            </div>

<!doctype html>
<html lang="en">

<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title><%- pageTitle %></title>
    <link rel="shortcut icon" type="image/png" href="/img/logo/icon.png"/>
    <link rel="stylesheet" href="/css-admin/styles.css"/>
</head>

<body>
<!--  Body Wrapper -->
<div class="page-wrapper" id="main-wrapper" data-layout="vertical" data-navbarbg="skin6" data-sidebartype="full"
     data-sidebar-position="fixed" data-header-position="fixed">
    <!-- Sidebar Start -->
    <%- include('sidebar.ejs') %>
    <!--  Sidebar End -->
    <!--  Main wrapper -->
    <div class="body-wrapper">
        <!--  Header Start -->
        <%- include('header.ejs') %>
        <!--  Header End -->
        <div class="container-fluid">
            <!--  Row 1 -->
            <div class="row">
                <div class="col-lg-10 d-flex">
                </div>
                <div class="col-lg-2 d-flex">
                    <select id="selection-id" class="form-select">

                    </select>
                </div>
            </div>
            <div class="row">
                <div class="col-lg-12 d-flex align-items-stretch">
                    <div class="card w-100">
                        <div class="card-body p-4">
                            <h5 id="data-type" class="card-title fw-semibold mb-4"><%- dataTitle %></h5>
                            <div class="table-responsive">
                                <table class="table text-nowrap mb-0 align-middle">
                                    <thead class="text-dark fs-4">
                                    <tr id="header-row">

                                    </tr>
                                    </thead>
                                    <tbody id="data-rows">
                                    </tbody>
                                </table>
                            </div>
                        </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>


        <div class="py-6 px-6 text-center">
            <p class="mb-0 fs-4">Developed by <a href="https://www.github.com/1brahimmohamed" target="_blank"
                                                 class="pe-1 text-primary text-decoration-underline">Ibrahim Mohamed</a>
            </p>
        </div>

    </div>
</div>
<script type="text/javascript" src="/libs-admin/jquery/dist/jquery.min.js"></script>
<script type="text/javascript" src="/libs-admin/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
<script type="text/javascript" src="/js-admin/sidebarmenu.js"></script>
<script type="text/javascript" src="/js-admin/app.min.js"></script>
<script type="text/javascript" src="/libs-admin/apexcharts/dist/apexcharts.min.js"></script>
<script type="text/javascript" src="/libs-admin/simplebar/dist/simplebar.js"></script>

<script type="text/javascript" src="/js-admin/dataRendering.js"></script>
</body>

</html>